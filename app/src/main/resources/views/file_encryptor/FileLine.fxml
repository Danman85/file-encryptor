<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>


<HBox fx:id="root" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.danman85.file_encryptor.client.views.file_encryptor.FileLineController">
   <children>
          <Label prefWidth="400.0" text="File">
              <font>
                  <Font size="14.0" />
              </font>
              <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
              </HBox.margin>
          </Label>
          <Region HBox.hgrow="ALWAYS" />
          <Button minWidth="-Infinity" mnemonicParsing="false" text="Encrypt">
              <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
              </HBox.margin>
          </Button>
          <Button minWidth="-Infinity" mnemonicParsing="false" text="Decrypt">
              <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
              </HBox.margin>
          </Button>
          <Button minWidth="-Infinity" mnemonicParsing="false" text="Close">
              <HBox.margin>
                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
              </HBox.margin>
          </Button>
   </children>
</HBox>
